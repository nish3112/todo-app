<.header>
  Listing Todos 
  <:actions>
    <.link patch={~p"/todos/new"}>
      <.button>New Todo</.button>
    </.link>
  </:actions>
</.header>




<form phx-change="search" id="search" >
  <input class = "search-bar" type="search" placeholder="Search..." name="title" phx-debounce="500">
</form>


<div class="bookmark">
  <%= if @bookmark do %>
  <button phx-click="bookmark" phx-value="{phx_value}">
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark-fill" viewBox="0 0 16 16">
      <path d="M2 2v13.5a.5.5 0 0 0 .74.439L8 13.069l5.26 2.87A.5.5 0 0 0 14 15.5V2a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2"/>
    </svg>
  </button>
  <% else %>
    <button phx-click="bookmark" phx-value="{phx_value}">
      <svg style="color: rgb(0, 0, 0);" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark" viewBox="0 0 16 16"> <path d="M2 2a2 2 0 0 1 2-2h8a2 2 0 0 1 2 2v13.5a.5.5 0 0 1-.777.416L8 13.101l-5.223 2.815A.5.5 0 0 1 2 15.5V2zm2-1a1 1 0 0 0-1 1v12.566l4.723-2.482a.5.5 0 0 1 .554 0L13 14.566V2a1 1 0 0 0-1-1H4z" fill="#000000"></path> </svg>
    </button>
  <% end %>  
</div>




<div class="card-region" id="card-region" phx-update="stream">
  <%= for {id,todo} <- @streams.todos do %>
    <div class="card shadow" id={id}>
      <img src="data:image/jpeg;base64," alt="FF ">
      <p class="title">Title: <%= todo.title %> </p>
      <p class="body">Body: <%= todo.body %></p>
      <p class="status">Status: <%= todo.status %> </p>
      <button phx-click="togglelike" phx-value-todo_id= {todo.id}>
        <%= if todo.liked do %>
        <svg class="w-6 h-6 text-gray-800 dark:text-black" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="orange" viewBox="0 0 24 24">
          <path d="m12.7 20.7 6.2-7.1c2.7-3 2.6-6.5.8-8.7A5 5 0 0 0 16 3c-1.3 0-2.7.4-4 1.4A6.3 6.3 0 0 0 8 3a5 5 0 0 0-3.7 1.9c-1.8 2.2-2 5.8.8 8.7l6.2 7a1 1 0 0 0 1.4 0Z"/>
        </svg>
        
        <% else %>
          <svg class="w-6 h-6 text-gray-800 dark:text-black" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24">
            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6C6.5 1 1 8 5.8 13l6.2 7 6.2-7C23 8 17.5 1 12 6Z"/>
          </svg>
         <% end %>
      </button>
      
     <!-- <.link navigate={~p"/todos/#{todo}"}>Show</.link> -->
      
      <.link patch={~p"/todos/#{todo}/edit"}>
        <svg width="24" height="24" stroke-width="1.5" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"> <path d="M13.0207 5.82839L15.8491 2.99996L20.7988 7.94971L17.9704 10.7781M13.0207 5.82839L3.41405 15.435C3.22652 15.6225 3.12116 15.8769 3.12116 16.1421V20.6776H7.65669C7.92191 20.6776 8.17626 20.5723 8.3638 20.3847L17.9704 10.7781M13.0207 5.82839L17.9704 10.7781" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"/> </svg>
      </.link>
      
      <.link
        phx-click={JS.push("delete", value: %{id: todo.id}) |> hide("##{id}")}
        data-confirm="Are you sure?"
      >
      <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M7 4a2 2 0 0 1 2-2h6a2 2 0 0 1 2 2v2h4a1 1 0 1 1 0 2h-1.069l-.867 12.142A2 2 0 0 1 17.069 22H6.93a2 2 0 0 1-1.995-1.858L4.07 8H3a1 1 0 0 1 0-2h4V4zm2 2h6V4H9v2zM6.074 8l.857 12H17.07l.857-12H6.074z" fill="#0D0D0D"/></svg>
      </.link>

    </div>
  <% end %>
  </div>
  

  <div class="pagination">
   <.button phx-click="previous" phx-value-id={@page_number}>Previous</.button>
   &nbsp;&nbsp;
   <%= @page_number + 1 %>
   &nbsp;&nbsp;
  <.button phx-click="next" phx-value-id={@page_number}>Next</.button> 
</div>






  <.modal :if={@live_action in [:new, :edit]} id="todo-modal" show on_cancel={JS.patch(~p"/todos")}>
    <.live_component
      module={TodoAppFullWeb.TodoLive.FormComponent}
      current_user={@session_id}
      id={@todo.id || :new}
      title={@page_title}
      action={@live_action}
      todo={@todo}
      patch={~p"/todos"}
    />
  </.modal>
  
  
  